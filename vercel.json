{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "functions": {
    "app/work/[slug]/page.js": {
      "maxDuration": 30
    },
    "app/work/page.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/public/images/(.*)",
      "destination": "/images/$1",
      "permanent": true
    }
  ]
}
